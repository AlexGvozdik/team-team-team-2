<div class="team-modal">
    <div class='dev-team'>
        <h2>Developer's Team</h2>
    </div>

    <ul class='dev-list'>
        {{#each this}}
        <li class='dev-items'>
            <img src='{{this.img}}' alt='student'/>
            <div class='dev-info'>
            <b class='dev-name'>{{this.name}}</b>
            <p class='position'>{{this.position}}</p>
            <ul class='social-link list'>
                <li class='link-item'>
                    <a href="{{this.githubLink}}" target="_blank" rel="noopener noreferrer"><svg class="card__icon" width="30"
                            height="30" style="fill:black">
                            <use href="sprite.svg#icon-github"></use>
                        </svg></a>
                </li>
                <li class='link-item'>
                    <a href="{{this.linkedInLink}}" target="_blank" rel="noopener noreferrer"><svg class="card__icon" width="30" height="30" style="fill:blue">
                            <use href="sprite.svg#icon-linkedin"></use>
                        </svg></a>
                </li>
            </ul>
        </li>
        {{/each}}
    </ul>
</div>